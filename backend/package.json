{
  "name": "partners-api",
  "version": "1.0.0",
  "description": "Backend for partners API",
  "main": "./dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "debug": "nodemon",
    "start:prod": "node dist/index.js",
    "lint": "./node_modules/.bin/tslint --project ./tsconfig.json -e 'src/test/**/*.*'",
    "lint:fix": "./node_modules/.bin/tslint --project ./tsconfig.json -e 'src/test/**/*.*'  --fix",
    "test": "ENV=test nyc mocha -r ts-node/register -r dotenv/config src/test/**/*.spec.ts --exit",
    "coverage": ". && nyc -r text-summary -r html npm run test"
  },
  "keywords": [
    "typescript",
    "template"
  ],
  "license": "ISC",
  "nyc": {
    "cache": false,
    "check-coverage": false,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "coverage/**",
      "src/test/**",
      "src/config/environments",
      "src/lib/interfaces",
      "node_modules/**"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": false,
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true,
    "instrument": true
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "core-decorators": "^0.20.0",
    "cors": "^2.8.5",
    "date-fns": "^2.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.0.2",
    "fs-extra": "^10.1.0",
    "helmet": "^4.6.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "nodemon": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "strong-params": "^1.0.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.8.3",
    "url-slug": "^3.0.2",
    "winston": "^3.2.1",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.29",
    "@types/chai": "^4.2.6",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.2",
    "@types/express-fileupload": "^1.1.7",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/lodash": "^4.14.171",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.20.15",
    "@types/validator": "^12.0.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-spies": "^1.0.0",
    "concurrently": "^5.0.0",
    "istanbul": "^0.4.5",
    "mime": "^2.5.2",
    "mime-types": "^2.1.31",
    "mocha": "^6.2.3",
    "nock": "^11.7.0",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "ts-sinon": "^1.0.24"
  }
}
